CREATE DATABASE phpmysql;
USE phpmysql;

CREATE TABLE phpmysql(
    name VARCHAR(50),
    lastname VARCHAR(100),
    email VARCHAR(100) NOT NULL UNIQUE,
    age INT NOT NULL,
    job VARCHAR(100)
);

